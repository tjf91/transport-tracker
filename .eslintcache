ceipt}\r\n                setFormToggle={setFormToggle} \r\n                    />                    \r\n            } \r\n            \r\n            <MapboxGL\r\n            formReceipt={formReceipt}\r\n            setFormReceipt={setFormReceipt}\r\n            receipts={receipts} \r\n            d_id={props.d_id}\r\n            />            \r\n            <div className='pie-chart'>\r\n            <Pie receipts={receipts}\r\n            margin={{ top: 30, right: 0, bottom: 30, left: 0 }}/>\r\n            </div>\r\n            <div className='bar-chart'>\r\n            <Bar \r\n            company_id={props.match.params.company_id}\r\n            driver_d_id={props.match.params.driver_d_id}\r\n            trip_id={props.match.params.trip_id}\r\n            receipts={receipts}\r\n            margin={{ top: 50, right: 130, bottom: 50, left: 60 }}\r\n            />\r\n            </div>\r\n            {mappedReceipts}             \r\n        </div>\r\n    )\r\n    }\r\n\r\nfunction mapStateToProps(reduxState){\r\n    return reduxState\r\n}\r\nexport default connect(mapStateToProps)(withRouter(Trip))","S:\\Projects\\transport-tracker\\src\\components\\Forms\\ReceiptForm.js",["96"],"S:\\Projects\\transport-tracker\\src\\components\\Forms\\TripForm.js",[],"S:\\Projects\\transport-tracker\\src\\components\\MapboxGL\\MapboxGL.js",["97","98","99","100","101","102","103","104","105"],"S:\\Projects\\transport-tracker\\src\\components\\Receipt\\Receipt.js",["106","107"],"S:\\Projects\\transport-tracker\\src\\components\\Charts\\Pie.js",["108"],"S:\\Projects\\transport-tracker\\src\\components\\Charts\\Bar.js",["109"],"S:\\Projects\\transport-tracker\\src\\components\\Charts\\options.js",[],"S:\\Projects\\transport-tracker\\src\\components\\Charts\\barFunctions.js",[],{"ruleId":"110","replacedBy":"111"},{"ruleId":"112","replacedBy":"113"},{"ruleId":"114","severity":1,"message":"115","line":37,"column":7,"nodeType":"116","endLine":37,"endColumn":9,"suggestions":"117"},{"rule